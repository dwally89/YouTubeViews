<Window x:Class="YouTube_Views_WPF.InputBoxWPF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Views" Height="104" Width="447" Icon="/YouTube%20Views%20WPF;component/Images/YouTube.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Enter in number of views for &quot;insert song name here&quot;" Height="28" HorizontalAlignment="Left" Name="lblDisplay" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="296,2,0,0" Name="txtViews" VerticalAlignment="Top" Width="120" />
        <Button Content="Previous" Height="23" HorizontalAlignment="Left" Margin="176,31,0,0" Name="btnPrevious" VerticalAlignment="Top" Width="75" Click="btnPrevious_Click" />
        <Button Content="Next" Height="23" HorizontalAlignment="Left" Margin="257,31,0,0" Name="btnNext" VerticalAlignment="Top" Width="75" Click="btnNext_Click" IsDefault="True" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="338,31,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" IsCancel="True" />
    </Grid>
</Window>
